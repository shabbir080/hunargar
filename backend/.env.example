# Copy this file to .env and fill with real values
# Mailtrap API token (required for sending emails in development)
MAILTRAP_API_TOKEN=your_mailtrap_api_token_here

# Email address where admin/contact messages should be sent
SUPPORT_EMAIL=admin@example.com

# Optional server settings
PORT=5000
NODE_ENV=development

# Frontend origin allowed by CORS (used by server)
CLIENT_ORIGIN=http://localhost:5173
